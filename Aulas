package lp2;
import java.util.Scanner;
public class Arss {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		float num=0;
		
		for(int i=0; i<10; i++) {
			System.out.println("Insira"+i+"inteiro");
			num +=sc.nextInt();
		}
		System.out.println("Media:"+(num/10));
	}

}
